<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
           <% if(cartProducts.length > 0) { %> 
                <ul>
                    <% for(product of cartProducts) { %>
                    <li><%= product.productData.title %> (<%= product.qty %>)</li>
                    <% } %>
                </ul>
            <% } else { %>
                <h2>No Products in Cart</h2>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>